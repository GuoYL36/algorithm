/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    var sum=0;
    var N=[];
    for (var i=0;i<nums.length-1;i++) {
        sum=nums[i]+nums[i+1];
        if (sum == target) {
            N=[i,i+1];
            return N
        }
    }
    //console.log(N);
};
